<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
      "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xdsoft.mvc.mapper.LoginUserOrderListMapper">



	<!-- 新增表信息 -->
	<insert id="addInfo" parameterType="java.util.Map"
		useGeneratedKeys="true" keyProperty="uid">
		insert into
		loginuserorderlist(randordernum,userinfouid,timestamp)
		values(#{randordernum},#{userinfouid},#{timestamp})
	</insert>
	
	


</mapper>  