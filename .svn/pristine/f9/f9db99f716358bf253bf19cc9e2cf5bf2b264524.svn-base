@Controller
@RequestMapping(value = "/manager")
public class ManagerController {

	@PublicPage
	@RequestMapping(value = "/manager")
	public String manager(HttpServletRequest request,
			HttpServletResponse response) {
		return "/manager/manager";
	}
}
