<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper  
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
      "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xdsoft.mvc.mapper.PrintInfoMapper">

	<!-- 普通文件打印服务 -->
	<!-- 新增记录 -->
	<insert id="addPrintInfo" parameterType="java.util.Map"
		useGeneratedKeys="true" keyProperty="id">
		insert into
		printinfo(tempfile_id,filepages,option1,option2,copies,timestamp)
		values(#{tempfile_id},#{filepages},#{option1}, #{option2},
		#{copies},#{timestamp})
	</insert>

	<!--根据tempfileid查询唯一标识的ID -->
	<select id="queryPrintInfoByTempfileid" parameterType="java.lang.String"
		resultType="java.util.Map">
		select * from printinfo p
		where p.tempfile_id =
		#{tempfile_id}
	</select>

	<!-- 文库文件打印服务 -->
	<insert id="AddBunkoFilePrintInfo" parameterType="java.util.Map"
		useGeneratedKeys="true" keyProperty="id">
		insert into
		bunko_printinfo(bunko_file_id,ordernum_id,option1,option2,copies,timestamp)
		values(#{bunko_file_id},#{ordernum_id},#{option1}, #{option2},
		#{copies},#{timestamp})
	</insert>

</mapper>  